 <header>
        <img src="/assets/graphics/Wacky Tales Logo.png" alt="Wacky Tales Logo">
    </header>
    <div class="content">
        <h1>Create Account</h1>
        <form id="create-account-form">
            <label for="user-name">Username</label>
            <input type="text" id="user-name" name="user-name" required>

            <label for="email">Email</label>
            <input type="email" id="email" name="email" required>

            <label for="password">Password</label>
            <input type="password" id="password" name="password" required>

            <div class="buttons">
                <button type="submit">Create Account</button>
            </div>
        </form>             
  </div>
</div>
<img src="/assets/graphics/create-acct-clown.png" alt="Wacky Tales Clown" class="create-clown-image">
<img src="/assets/graphics/Clown Head Sticker.png" alt="Wacky Tales Clown" class="create-clown-left">
<script src="/js/createAccount.js"></script>